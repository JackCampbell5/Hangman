<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jdev.hangman">

    <application
        android:allowBackup="true"
        android:icon="@drawable/hangmanforhome"
        android:label="@string/app_name"
        android:roundIcon="@drawable/hangmanforhome"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.jdev.hangman.settings"
            android:screenOrientation="portrait"
            ></activity>
        <activity
            android:name="com.jdev.hangman.enter_Code"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.jdev.hangman.level_select"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.jdev.hangman.help_screen"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.jdev.hangman.first_level"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.jdev.hangman.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>